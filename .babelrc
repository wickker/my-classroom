{
  "presets": [
    ["@babel/preset-env",
    {
      "targets": {
        "browsers": [
          ">0.25%",
          "not ie 11",
          "not op_mini all"
        ]
      }
    }],
    "@babel/preset-react"
  ],
  "retainLines": true,
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel"
  ]
}
